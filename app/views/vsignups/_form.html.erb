<%= simple_form_for(@vsignup) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :User %>
    <%= f.association :Responsibility %>
    <%= f.association :Osignup %>
    <%= f.input :date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
